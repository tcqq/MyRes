<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </com.google.android.material.appbar.AppBarLayout>

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/client_register"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Client Register"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/login"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Login"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/client_home"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Client Home"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/restaurant_detail"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Restaurant Detail"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/menu"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Menu"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/order_tracking"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Order Tracking"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/payment"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Payment"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/review"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Review"
                    android:textAppearance="?attr/textAppearanceBody1"
                    android:textColor="?attr/colorPrimary" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/service_request"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Service Request"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/owner_home"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Owner Home"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/add_restaurant"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Add Restaurant"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/update_restaurant"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Update Restaurant"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/menu_management"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Menu Management"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/add_waitress"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Add Waitress"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/add_kitchen_worker"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Add Kitchen Worker"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/view_reviews"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="View Reviews"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/waitress_home"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Waitress Home"
                    android:textAppearance="?attr/textAppearanceBody1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/kitchen_worker_home"
                    android:layout_width="match_parent"
                    android:layout_height="64dp"
                    android:background="?attr/selectableItemBackground"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:text="Kitchen Worker Home"
                    android:textAppearance="?attr/textAppearanceBody1" />

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</layout>